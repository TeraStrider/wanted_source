"lightmappedgeneric"
{
	"$basetexture" "wanted/!bluwter"
//	"$texture2"	"liquids/toxic_anim"
	"$texture2" 	"wanted/!bluwter"	
	"$texture2scale" 1
//	"$additive" 1

	"$tex2offset"	"[0 0]"
	"%compilewater" 1

	"$bottommaterial" "wanted/!bluwter"

	"$surfaceprop" "water"
	"$fogenable" 1
	"$fogcolor" "{96 255 0}"
	"$fogstart" 1.00
	"$fogend" 400.00

	"Proxies"
	{
		"LinearRamp"
		{
			"resultVar" "$tex2offset[0]"
			"rate" 0.001
		}
		"LinearRamp"
		{
			"resultVar" "$tex2offset[1]"
			"rate" -0.01
		}
		"TextureTransform"
		{
			"translateVar"	"$tex2offset"
			"resultVar"	"$texture2transform"
		}
	}
}